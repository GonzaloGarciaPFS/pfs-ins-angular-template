{
  "name": "pfs-ins-zbo-dashboard-webapp",
  "version": "1.0.0",
  "scripts": {
    "ng": "npx ng",
    "serve": "npx ng serve --host=0.0.0.0",
    "start": "npm run serve",
    "build": "npx ng build --build-optimizer --stats-json --source-map=false",
    "test": "npx ng test",
    "lint": "npx ng lint ebox-webapp",
    "e2e": "npx ng e2e",
    "compodoc": "npx compodoc -p tsconfig.doc.json -s --theme laravel",
    "start:doc": "npx compodoc -s",
    "start:electron": "npx electron .",
    "start:desa-insurance": "npm run serve -- -c desa-insurance --proxy-config proxy-desa-insurance.conf.json",
    "start:inte-insurance": "npm run serve -- -c inte-insurance --proxy-config proxy-inte-insurance.conf.json",
    "start:demo-insurance": "npm run serve -- -c demo-insurance --proxy-config proxy-demo-insurance.conf.json",
    "start:ebox-zbo-demo": "npm run serve -- -c ebox-zbo-demo --proxy-config proxy-ebox-zbo-demo.conf.json",
    "build:desa-insurance": "npm run build -- -c desa-insurance",
    "build:inte-insurance": "npm run build -- -c inte-insurance",
    "build:demo-insurance": "npm run build -- -c demo-insurance",
    "build:ebox-zbo-demo": "npm run build -- -c ebox-zbo-demo",
    "docker-build:desa-insurance": "export ENV_NAME=desa-insurance && export URL_PROXY=192.168.70.66 && docker-compose down && docker-compose build --no-cache && docker-compose up -d",
    "docker-build:inte-insurance": "export ENV_NAME=inte-insurance && export URL_PROXY=192.168.70.133 && docker-compose down && docker-compose build --no-cache && docker-compose up -d",
    "docker-build:demo-insurance": "export ENV_NAME=demo-insurance && export URL_PROXY=192.168.71.50 && docker-compose down && docker-compose build --no-cache && docker-compose up -d",
    "docker-build:ebox-zbo-demo": "export ENV_NAME=ebox-zbo-demo && export URL_PROXY=192.168.70.147 && docker-compose down && docker-compose build --no-cache && docker-compose up -d",
    "doc:buildandserve": "npx compodoc -s -p tsconfig.app.json",
    "doc:json": "compodoc -p ./tsconfig.json -e json -d .",
    "prepare": "husky install",
    "eslint-issues": "npx eslint $(git status --porcelain | cut -c4-)"
  },
  "lint-staged": {
    "*.ts": "eslint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.1.0",
    "@angular/cdk": "~14.1.0",
    "@angular/common": "^14.1.0",
    "@angular/compiler": "^14.1.0",
    "@angular/core": "^14.1.0",
    "@angular/flex-layout": "^14.0.0-beta.40",
    "@angular/forms": "^14.1.0",
    "@angular/localize": "^14.1.0",
    "@angular/material": "^14.1.0",
    "@angular/material-moment-adapter": "^14.1.0",
    "@angular/platform-browser": "^14.1.0",
    "@angular/platform-browser-dynamic": "^14.1.0",
    "@angular/router": "^14.1.0",
    "@angular/service-worker": "^14.1.0",
    "@auth0/angular-jwt": "^5.0.2",
    "@ckeditor/ckeditor5-angular": "^2.0.2",
    "@ng-select/ng-select": "^9.0.2",
    "@ngx-translate/core": "^11.0.0",
    "@ngx-translate/http-loader": "^4.0.0",
    "autoprefixer": "10.4.5",
    "bootstrap": "4.6.1",
    "file-saver": "^2.0.5",
    "filepond": "^4.30.4",
    "filepond-plugin-file-encode": "^2.1.10",
    "filepond-plugin-file-validate-size": "^2.2.7",
    "filepond-plugin-file-validate-type": "^1.2.8",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "ng2-pdf-viewer": "^6.4.1",
    "ngx-bootstrap": "^5.6.2",
    "ngx-filepond": "^5.0.1",
    "ngx-mask": "10.0.4",
    "ngx-quill": "^18.0.0",
    "quill": "^1.3.7",
    "rxjs": "~7.5.6",
    "tslib": "^1.14.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~14.1.0",
    "@angular/cli": "^14.1.0",
    "@angular/compiler-cli": "^14.1.0",
    "@angular/language-service": "^14.1.0",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^14.14.31",
    "codelyzer": "^6.0.2",
    "jasmine-core": "~3.8.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.7.0",
    "protractor": "~7.0.0",
    "typescript": "~4.7.4",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "eslint": "^8.19.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.4",
    "eslint-plugin-promise": "^6.0.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3"
  }
}
